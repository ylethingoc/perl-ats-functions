    </desc>
                <paramset>
        <param porttype="2"></param>
        <param esfd4="1"></param>
        <param linecode="1"></param>
        <param clocksource="2"></param>
        <param T1app="1"></param>
                </paramset>
  </set>
  <set confset="J7 PS0204">
    <desc>
      This set contains the list of parameters needed for configuring J7 protocol with board type PS0204.
    </desc>
                <paramset>
        <param porttype="1"></param>
        <param linecode="1"></param>
                </paramset>
  </set>
  <set confset="J7 PH0301">
    <desc>
      This set contains the list of parameters needed for configuring J7 protocol with board type PH0301.
    </desc>
                <paramset>
        <param linktype="1"></param>
